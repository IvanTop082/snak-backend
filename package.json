{
  "name": "@kasarlabs/snak",
  "version": "0.0.10",
  "private": true,
  "description": "Build powerful and secure agents powered by Starknet",
  "author": "Kasar Labs",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasarlabs/snak.git"
  },
  "packageManager": "pnpm@9.3.0",
  "keywords": [
    "starknet",
    "blockchain",
    "AI",
    "agent"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist node_modules",
    "build": "tsc",
    "start:server": "ts-node server.ts",
    "dev": "ts-node-dev --respawn server.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --check \"**/*.{ts,tsx}\" --write",
    "test:server": "ts-node test-server.ts"
  },
  "dependencies": {
    "@brian-ai/sdk": "^0.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.18.2",
    "starknet": "^6.23.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.5",
    "eslint": "^9.22.0",
    "prettier": "^3.5.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "overrides": {
      "tough-cookie": "^4.1.3"
    }
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/kasarlabs/snak/issues"
  },
  "homepage": "https://github.com/kasarlabs/snak#readme"
}
